{
    "name": "Regression",

    "model": {
        "type": "Model",
        "args": {
            "pipeline": ["column_transformer", "XGB"],
            "unions": {
            }
        }
    },

    "tuned_parameters": [
        {
            "XGB__objective": [
                "reg:squarederror"
            ],
            "XGB__eval_metric": [
                "mae"
            ],
            "XGB__n_estimators": [
                100
            ],
            "XGB__learning_rate": [
                0.300000012
            ],
            "XGB__gamma": [
                0,
                0.1
            ],
            "XGB__alpha": [
                0,
                0.1
            ],
            "XGB__max_depth": [
                5
            ],
            "XGB__use_label_encoder": [
                false
            ]
        }
    ],

    "optimizer": "OptimizerRegression",

    "search_method":{
        "type": "RandomizedSearchCV",
        "args": {
            "n_iter": 1,
            "refit": false,
            "n_jobs": 1,
            "verbose": 2,
            "error_score": 0
        }
    },

    "cross_validation": {
        "type": "RepeatedKFold",
        "args": {
            "n_splits": 5,
            "n_repeats": 1,
            "random_state": 1
        }
    },

    "data_loader": {
        "type": "TFT_Challengers",
        "args":{
            "data_path": "na1_challengers_12.14.456.5556_3days_matches",
            "shuffle": true,
            "test_split": 0.20,
            "stratify": false,
            "random_state":1
        }
    },
    "label_name": "placement",
    "score": "max neg_mean_absolute_error",
    "train_model": true,
    "test_model": true,
    "debug": false,
    "save_dir": "saved/",
    "model_dir": ""

}
