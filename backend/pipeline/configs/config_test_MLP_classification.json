{
    "name": "Classification",
    "model": {
        "type": "Model",
        "args": {
            "pipeline": [
                "scaler",
                "MLPClass"
            ],
            "unions": {}
        }
    },
    "tuned_parameters": [
        {
            "MLPClass__hidden_layer_sizes": [
                [50], [100], [200]
            ],
            "MLPClass__optimizer__learning_rate": [
                0.0001, 0.001, 0.1
            ]
        }
    ],
    "optimizer": "OptimizerClassification",
    "search_method": {
        "type": "GridSearchCV",
        "args": {
            "refit": false,
            "n_jobs": -1,
            "verbose": 2,
            "error_score": 0
        }
    },
    "cross_validation": {
        "type": "RepeatedStratifiedKFold",
        "args": {
            "n_splits": 5,
            "n_repeats": 5,
            "random_state": 42
        }
    },
    "data_loader": {
        "type": "TestKerasClassification",
        "args": {
            "data_path": "",
            "shuffle": true,
            "test_split": 0.2,
            "stratify": true,
            "random_state": 42
        }
    },
    "label_name": "",
    "score": "max balanced_accuracy",
    "train_model": true,
    "test_model": true,
    "debug": false,
    "save_dir": "saved/",
    "model_dir": ""
}